<jaxb:bindings xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
          xmlns:xsi="http://www.w3.org/2000/10/XMLSchema-instance"
          xmlns:xs="http://www.w3.org/2001/XMLSchema"
          version="2.1">
  
  <jaxb:globalBindings>
       
       <!-- Map datetime values to string to enable custom parsing -->
       <jaxb:javaType name="java.lang.String" xmlType="xs:dateTime"/>
       
  </jaxb:globalBindings>
  
</jaxb:bindings>
